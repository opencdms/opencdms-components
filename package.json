{
  "name": "opencdms-components",
  "version": "0.2.1",
  "license": "SEE LICENSE in LICENSE",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:examples": "npx browser-sync start --server --watch --startPath examples --files 'components/ examples/'",
    "build": "ng build",
    "build:components": "ng build --configuration production,webcomponents && node scripts/concat-bundles.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:types:opencdms": "npx openapi-typescript https://api.opencdms.org/climsoft/openapi.json --output src/models/opencdms-api.ts",
    "generate:types:components": "npx openapi-typescript https://opencdms-components-api-jnxkyuyk3q-ew.a.run.app/openapi.json --output src/models/components-api.ts"
  },
  "dependencies": {
    "@angular/animations": "^13.3.0",
    "@angular/cdk": "^13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/elements": "^13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "^13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@coreui/angular": "4.0.0-beta.2",
    "@coreui/coreui": "^4.1.3",
    "@coreui/icons": "^2.1.0",
    "@coreui/icons-angular": "^3.0.0-alpha.4",
    "file-saver": "^2.0.5",
    "openapi-typescript-fetch": "^1.1.2",
    "papaparse": "^5.3.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^16.11.26",
    "@types/papaparse": "^5.3.2",
    "concat": "^1.0.3",
    "fs-extra": "^10.0.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  },
  "packageManager": "yarn@3.2.0"
}
